{
  "name": "devolympus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "clean": "rm -rf .next && rm -rf node_modules",
    "ins": "pnpm install && prisma generate",
    "ins:clean": "pnpm clean && pnpm install && prisma generate",
    "dev:clean": "pnpm ins:clean && pnpm run dev",
    "build": "next build",
    "build:clean": "pnpm ins:clean && pnpm run build",
    "start": "next start",
    "serve": "pnpm build && pnpm start",
    "serve:clean": "pnpm build:clean && pnpm start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,mjs,js,jsx,md,mdx,css,yaml,yml}\"",
    "check": "prettier --check \"**/*.{ts,tsx,mjs,js,jsx,md,mdx,css,yaml,yml}\"",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "6.5.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.6.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.4.7",
    "intersection-observer": "^0.12.2",
    "lucide-react": "^0.477.0",
    "next": "15.2.0",
    "next-auth": "5.0.0-beta.25",
    "next-cloudinary": "^6.16.0",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "react": "^19.0.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.15.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
