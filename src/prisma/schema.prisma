generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  name      String?
  email     String  @unique
  image     String?
  loginJson Json
  isBlocked Boolean @default(false)
  isAdmin   Boolean @default(false)

  @@unique([id])
}
